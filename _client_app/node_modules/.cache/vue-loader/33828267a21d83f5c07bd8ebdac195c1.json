xt-field label=\"Data\" v-model=\"item.action\"></v-text-field>\n                </v-col>\n              </v-row>\n            </v-container>\n          </v-card-text>\n          <v-card-actions>\n            <v-spacer></v-spacer>\n            <v-btn color=\"blue darken-1\" text @click=\"closeModal\">Close</v-btn>\n            <v-btn color=\"blue darken-1\" text @click=\"addOrUpdateTodo\">Save</v-btn>\n          </v-card-actions>\n        </v-card>\n      </v-dialog>\n    </v-row>\n  </div>\n</template>\n\n<script>\nexport default {\n  props: {\n      item: {\n        id: \"\",\n        titulo: \"\",\n        subtitulo: \"\",\n        action: \"\",\n        completed: \"\"\n      },\n  },\n  created(){\n    console.log('modal status: '+this.modalstatus)\n    console.log('editing status: '+this.editingstatus)\n  },\n  data() {\n    return {\n      newItem: {\n        id: \"\",\n        titulo: \"\",\n        subtitulo: \"\",\n        action: \"\",\n        completed: \"\"\n      }\n    };\n  },\n  computed:{\n    modalstatus(){\n      return this.$store.state.modal_active\n    },\n    editingstatus(){\n      return this.$store.state.editing\n    }\n  },\n  methods: {\n\n    addOrUpdateTodo() {\n      if (this.item.length == 0) {\n        return;\n      }\n\n      if(this.$store.state.editing === true){\n        this.$store.dispatch(\"updateTodo\", this.item);\n      }\n      \n      if(this.$store.state.editing === false){\n        this.$store.dispatch(\"addTodo\", this.item);\n      }\n\n      this.closeModal();\n    },\n    closeModal() {\n      this.$store.state.modal_active = false;\n      this.$store.state.editing = false;\n    }\n  }\n};\n</script>"]}]}